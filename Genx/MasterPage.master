                            k.target.parentNode.parentNode.parentNode.parentNode.style.overflowY = 'auto';
                            levelOneBack.className += ' closeBackOne';
                            setTimeout(removeCloseOneCss, 600);
                        })
                    })
                }

                //..................secon step back
                function backArrowTwo() {
                    Array.from(backLevelTwo).forEach(function (backTwo) {
                        backTwo.addEventListener('click', function (v) {
                            parntHeight.style.overflowY = 'auto';
                            levelTwoBack = v.target.parentNode.parentNode;
                            levelTwoBack.className += ' closeBackOne';
                            // levelTwoBack.classList.remove('rmm-subview');
                            setTimeout(removeCloseTwoCss, 600);
                        })
                    })
                }

                function removeCloseOneCss() {
                    levelOneBack.className = 'level margin-left_4 padding-top6 second rmm-submenu ';
                }

                function removeCloseTwoCss() {
                    levelTwoBack.className = 'menuTopSpacing rmm-submenu ';
                }

                //................................Swipe Slider................................................

                var _startX = 0;
                var _offsetX = 0;
                var boxT, boxk;
                var currentX = 0;
                var diff;
                mainMenu.ontouchstart = downFunction;

                function downFunction(e) {
                    mainMenu.ontouchmove = moveFunction;
                    diff = 300 - (e.touches ? e.touches[0].pageX : e.pageX);
                }

                function moveFunction(e) {
                    mainMenu.ontouchend = upFunction;
                    boxT = document.getElementById('mainMenu');
                    boxT.classList.remove('put');
                    currentX = e.touches ? e.touches[0].pageX : e.pageX;
                    boxT.style.left = (((currentX + diff) - 300) * 1.20) + 'px';

                    if ((currentX + diff) > 300) {
                        boxT.style.left = '0';
                    }
                }

                function upFunction(e) {
                    boxk = document.getElementById('mainMenu');
                    document.onmousemove = null;
                    boxT = null;
                    if ((currentX + diff) >= 120) {
                        boxk.classList.add('slide');
                        setTimeout(removeMax, 500);
                    } else if ((currentX + diff) <= 119) {
                        boxk.classList.add('removeS');
                        setTimeout(removeMin, 500);
                    }
                }

                function removeMax() {
                    boxk.style.left = '0';
                    boxk.classList.remove('slide');
                }

                function removeMin() {
                    boxk.style.left = "-310px";
                    boxk.classList.remove('removeS');
                    menuBlackout.style.display = 'none';
                    currentX = 150;
                    Array.from(slideLayers).forEach(function (closeOpenMenu) {
                        closeOpenMenu.className = 'level margin-left_4 padding-top6 second rmm-submenu';
                        window.document.body.style.overflowY = 'auto';
                    })
                    Array.from(slideLayers2).forEach(function (closeOpenMenu2) {
                        closeOpenMenu2.className = 'menuTopSpacing rmm-submenu';
                        window.document.body.style.overflowY = 'auto';
                    });
                    changeToAsItIs()
                }
                //..............................END swipw slider...........................................

                if (navbar != null) {
                    var sticky = navbar.offsetTop;
                }

                //...... bottom menu in mobile will show/hide when user scroll
                function scrollDetect() {
                    if (w < 992) {




                        var topNav = document.querySelector('.top-nav');
                        var ymDivCircle = document.querySelector('#ymDivCircle');
                        var lastScroll = 0;
                        window.onscroll = function () {
                            var currentScroll = document.documentElement.scrollTop || document.body.scrollTop; // Get Current Scroll Value
                            topNav.classList.remove("go");
                            topNav.classList.remove("come");
                            if (currentScroll > 0 && lastScroll <= currentScroll) {
                                lastScroll = currentScroll;
                                topNav.classList.add("go");
                                if (navbar) {
                                    myFunction(sticky);
                                }
                            } else {
                                lastScroll = currentScroll;
                                topNav.classList.add("come");
                                if (navbar) {
                                    myFunction(sticky);
                                }
                            }
                        };
                    }
                }

                scrollDetect();

                //...... product filter and sort fixed at to when user scroll
                function myFunction(sticky) {
                    if (window.pageYOffset >= sticky) {
                        navbar.classList.add("sticky");
                    } else {
                        navbar.classList.remove("sticky");
                    }
                }
                //....................Feature Tab Script//
                var checkClick = document.querySelector('#selectedTab');
                var floatMenuVar = document.querySelector('#floatMenu');
                var tabLink = document.querySelectorAll('#featuresTab li a')
                var currentLocation;

                if (checkClick != null) {
                    checkClick.addEventListener('click', function (c) {
                        document.getElementById('floatMenu').classList.toggle('linkHeight');
                        document.getElementById('arrowRotate').classList.toggle('rotate');
                    }, false);
                }

                window.addEventListener('click', function (e) {
                    if (checkClick != null) {
                        if (checkClick.contains(e.target)) {
                        } else {
                            document.getElementById('floatMenu').classList.remove('linkHeight');
                        }
                    }
                })

                Array.from(tabLink).forEach(function (g) {
                    g.addEventListener('click', function () {
                        document.getElementById('arrowRotate').classList.toggle('rotate');
                        document.getElementById('floatMenu').classList.remove('linkHeight');
                    })
                });

            });

        </script>

